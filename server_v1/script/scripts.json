{
    "name": "node_server",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "runNginx": "start powershell \"cd ./nginx/; start nginx;\"",
        "killNginx": "start powershell \"taskkill /F /IM nginx.exe /FI \\\"IMAGENAME eq nginx*\\\"\"",
        "createdFile": "node ./server/config/created_server_list/created_server_list.js",
        "runPm2": "pm2 start ./server/config/pm2_config/ecosystem.config.js",
        "stopPm2": "pm2 stop all && pm2 delete all",
        "start": "npm run createdFile && npm run runPm2",
        "reload": "npm run createdFile && pm2 reload all",
        "stop": "npm run stopPm2",
        "restart": "npm run stopPm2 && npm run createdFile && npm run runPm2"
    },
    "keywords": [],
