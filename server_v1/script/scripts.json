    "license": "ISC",
    "dependencies": {
        "compression": "^1.7.4",
        "express": "^4.18.1",
        "http-proxy-middleware": "^2.0.6",
        "log4js": "^6.6.1",
        "mime": "^3.0.0",
        "multer": "^1.4.4"
    }
}
