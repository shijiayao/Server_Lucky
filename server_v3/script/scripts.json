    "description": "",
    "main": "index.js",
    "scripts": {
        "server-v2": "pm2 start ./server_v2/config/pm2_config/ecosystem.config.js",
        "server-v3": "pm2 start ./server_v3/config/pm2_config/ecosystem.config.js",
        "runPm2": "pm2 start ./server_v2/config/pm2_config/ecosystem.config.js",
        "stopPm2": "pm2 stop all && pm2 delete all",
        "v2": "npm run server-v2",
        "v3": "npm run server-v3",
        "reload": "pm2 reload all",
        "stop": "npm run stopPm2",
        "restart": "npm run stopPm2 && npm run runPm2"
    },
